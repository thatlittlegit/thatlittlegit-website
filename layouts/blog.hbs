<?xml version="1.0" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html lang="en" dir="ltr">
	<head>
		<meta http-equiv="Content-Type" content="text/html; utf-8" />
		<title>blog</title>
		<meta http-equiv="Content-Security-Policy" content="default-src 'none'; style-src 'self' https://cdnjs.cloudflare.com; base-uri 'none'; form-action 'none'; frame-ancestors 'none';" />
		<link rel="manifest" href="manifest.json" />
		<link rel="stylesheet" href="/styles.css" />
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/styles/default.min.css" integrity="sha384-zhIsEafzyQWHSoMCQ4BfT8ZlRXQyIFwAHAJn32PNdsb8n6tVysGZSLpEEIvCskw4" crossorigin="anonymous" />
	</head>
	<body>
		<h1>blog</h1>
		[<a href="/">home</a>]&nbsp;[<a href="feed.xml">rss</a>]

		<ul>
			{{#each collections.posts}}
				<li><dl>
					<dt>{{this.date}}</dt>
					<dd><a href="/{{this.path}}">{{this.title}}</a></dd>
				</dl></li>
			{{/each}}
		</ul>
	</body>
</html>

